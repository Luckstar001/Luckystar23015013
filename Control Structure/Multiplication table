startnumb = int(input("Enter the starting number for the multiplication table: "))
if startnumb <= 0:
    print("Please enter a positive integer.")
else:
    print(f"Multiplication table starting from {startnumb}:")
    print()
    for i in range(startnumb, 13):
        for j in range(1, 13):
            product = i * j
            print(f"{i} x {j} = {product}")
        print()


